{
  "schemaVersion": 1,
  "id": "toolbelt",
  "version": "${version}",
  "name": "ToolBelt",
  "description": "A mod that adds a tool belt.",
  "authors": [
    "Jamalam"
  ],
  "contact": {
    "sources": "https://github.com/JamCoreModding/ToolBelt",
    "issues": "https://github.com/JamCoreModding/ToolBelt/issues"
  },
  "license": "MIT",
  "icon": "assets/toolbelt/textures/item/tool_belt.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.jamalam360.tool.belt.ToolBeltInit"
    ],
    "client": [
      "io.github.jamalam360.tool.belt.ToolBeltClientInit"
    ],
    "modmenu": [
      "io.github.jamalam360.tool.belt.config.ModMenuScreen"
    ]
  },
  "mixins": [
    "toolbelt.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.5",
    "fabric": "*",
    "jamlib": "*",
    "trinkets": "*"
  },
  "custom": {
    "notify_gradle_properties_url": "https://raw.githubusercontent.com/JamCoreModding/ToolBelt/main/gradle.properties",
    "notify_gradle_properties_key": "mod_version"
  }
}
